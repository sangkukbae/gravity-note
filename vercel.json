{
  "functions": {
    "app/**/*.ts": {
      "maxDuration": 30
    },
    "app/**/*.js": {
      "maxDuration": 30
    }
  },
  "headers": [
    {
      "source": "/_next/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/_next/app-build-manifest.json",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/json"
        },
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    }
  ],
  "rewrites": [],
  "redirects": []
}
